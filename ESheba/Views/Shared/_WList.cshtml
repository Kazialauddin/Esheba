@model IEnumerable<ESheba.Models.Worker>
@using ESheba.Models;
@{
    ViewBag.Title = "Workers";
}

<h2>Workers List </h2>
<div>
    <ul>
        @foreach (var w in Model)
        {
            <li>@Html.ActionLink(w.Name, "Details", new { Id = w.Id })</li>
        }
    </ul>
    @Html.ActionLink("Back", "DepartmentList", "Departments")
</div>


